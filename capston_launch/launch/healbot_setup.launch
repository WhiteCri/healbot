<launch>
  <node name="healbot_odometry" type="diff_odometry" pkg="healbot_odometry">
        <param name="wheel_radius" value="0.041"/>
        <param name="wheel_seperation" value="0.285"/>
        <param name="right_vel_topic_name" value="/right_motor_driver/filtered_velocity"/>
        <param name="left_vel_topic_name"  value="/left_motor_driver/filtered_velocity"/>
        <param name="odom_topic_name" value="/odom"/>
  </node>
    <node name="rplidarNode"          pkg="rplidar_ros"  type="rplidarNode" output="screen">
    <param name="serial_port"         type="string" value="/dev/ttyUSB1"/>
    <param name="serial_baudrate"     type="int"    value="115200"/><!--A1/A2 -->
    <!--param name="serial_baudrate"     type="int"    value="256000"--><!--A3 -->
    <param name="frame_id"            type="string" value="laser"/>
    <param name="inverted"            type="bool"   value="false"/>
    <param name="angle_compensate"    type="bool"   value="true"/>
  </node>

  <node pkg="tf" type="static_transform_publisher" name="lidar2base_link" args="0 0 0 0 0 0 base_link laser 100"/>
</launch>